{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true
    },
    "baseUrl": "./src_js/",
    "target": "es2022",
    "keepClassNames": true
  },
  "isModule": true,
  "minify": false,
  "module": {
    "type": "es6",
    "strict": true,
    "noInterop": true
  },
  "exclude": ["src_js/sample.ts"]
}
